{
    "expo": {
        "owner": "1023-ventures",
        "name": "acme-boats",
        "slug": "plio",
        "version": "0.1.0",
        "orientation": "portrait",
        "icon": "./assets/boat-icon-png-10.png",
        "plugins": [
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to let you share them with your friends."
                }
            ]
        ],
        "splash": {
            "image": "./assets/boat-icon-png-10.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "scheme": "plio",
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
